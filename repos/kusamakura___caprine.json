  "packageJSON": {
    "name": "caprine",
    "productName": "Caprine",
    "version": "2.0.0",
    "description": "Unofficial Facebook Messenger app",
    "license": "MIT",
    "repository": "sindresorhus/caprine",
    "author": {
      "name": "Sindre Sorhus",
      "email": "sindresorhus@gmail.com",
      "url": "sindresorhus.com"
    },
    "maintainers": [
      {
        "name": "Oskar Krawczyk",
        "email": "oskar.krawczyk@gmail.com",
        "url": "krawczyk.io"
      },
      {
        "name": "Daniel Pham",
        "email": "pham.dany@gmail.com",
        "url": "danhp.github.io"
      }
    ],
    "scripts": {
      "postinstall": "install-app-deps",
      "test": "xo",
      "start": "electron .",
      "pack": "build",
      "dist": "build",
      "dist:all": "build --platform=all"
    },
    "dependencies": {
      "electron-config": "^0.2.1",
      "electron-context-menu": "^0.8.0",
      "electron-debug": "^1.0.0",
      "electron-dl": "^1.0.0",
      "electron-is-dev": "^0.1.2",
      "electron-log": "^2.0.2",
      "electron-updater": "^1.3.2",
      "minimist": "^1.2.0"
    },
    "devDependencies": {
      "electron": "^1.6.5",
      "electron-builder": "^16.6.1",
      "xo": "*"
    },
    "xo": {
      "envs": [
        "node",
        "browser"
      ]
    },
    "build": {
      "files": [
        "**/*",
        "!media${/*}"
      ],
      "appId": "com.sindresorhus.caprine",
      "mac": {
        "category": "public.app-category.social-networking",
        "target": [
          "dmg",
          "zip",
          "mas"
        ]
      },
      "dmg": {
        "iconSize": 160,
        "contents": [
          {
            "x": 180,
            "y": 170
          },
          {
            "x": 480,
            "y": 170,
            "type": "link",
            "path": "/Applications"
          }
        ]
      },
      "linux": {
        "category": "Chat",
        "target": [
          "AppImage",
          "deb"
        ]
      },
      "win": {
        "target": [
          "nsis"
        ]
      }
    }
  },
